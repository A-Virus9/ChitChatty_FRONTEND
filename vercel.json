
  {
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "https://chit-chatty-frontend.vercel.app", 
                "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
                "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Content-Type"
            },
            "dest": "/api/index.js"
        }
    ],
}